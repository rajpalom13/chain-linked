{
  "name": "linkedin-data-extractor",
  "version": "4.0.0",
  "description": "Chrome extension for extracting LinkedIn profile data and analytics",
  "type": "module",
  "scripts": {
    "dev": "npm run build:watch",
    "build": "npm run build:popup && npm run build:scripts",
    "build:popup": "vite build",
    "build:scripts": "node esbuild.config.js",
    "build:watch": "concurrently \"vite build --watch\" \"node esbuild.config.js --watch\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "esbuild": "^0.19.0",
    "postcss": "^8.4.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.0",
    "tailwind-merge": "^2.2.0"
  }
}
